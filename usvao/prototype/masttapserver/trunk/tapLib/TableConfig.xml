              </Column>
              <Column name="RMagErr">
                <Description>Error in R Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="RMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="IMag">
                <Description>I Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="IMagErr">
                <Description>Error in I Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="IMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="JMag">
                <Description>J (IR) Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="JMagErr">
                <Description>Error in J Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="JMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="HMag">
                <Description>H Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="HMagErr">
                <Description>Error in H Magnitude</Description>
