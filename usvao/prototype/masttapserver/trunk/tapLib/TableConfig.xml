                <datatype>real</datatype>
              </Column>
              <Column name="UMagErr">
                <Description>Error in U Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="UMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="BMag">
                <Description>B Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="BMagErr">
                <Description>Error in B Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="BMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="RMag">
