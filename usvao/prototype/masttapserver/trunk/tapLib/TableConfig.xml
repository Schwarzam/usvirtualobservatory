<?xml version="1.0" encoding="utf-8"?>
<NewConfig>
    <Databases>
        <Database name="VOR10DEV" value="" >
            <!-- name required. type required. base_table, view, or output-->
            <!-- optional attribute UTYPE-->
            <Tables>
                <Table name="Resource" type="table">
                    <Description>Primary Base Table</Description>
                    <!-- meaning of "All" is determined by this entire listing. 
        Meaning of "std" is determined by columns with "std" attribute, set to true. Lack of
        "std" attribute is logically the same as "false"-->
					<Columns>
						<Column name="pkey" std="true">
							<Description>object ID, primary key</Description>
							<datatype>bigint</datatype>
							<primary>true</primary>
							<indexed>true</indexed>
						</Column>
						<Column name="title" >
							<Description>Brief Title of Resource</Description>
							<datatype>string</datatype>
						</Column>
						<Column name="identifier" >
							<Description>The IVORN of the resource</Description>
							<datatype>string</datatype>
						</Column>
						<Column name="shortName" >
							<Description>A few characters that describe the resource</Description>
							<datatype>string</datatype>
						</Column>
						<Column name="description" >
							<Description>Detailed description of resource content</Description>
							<datatype>string</datatype>
						</Column>			
					</Columns>
                 </Table>
            </Tables>
            <!-- Can have multiple tables. 
      Tables are logical tables from the client perspective, and may actually be views.-->
        </Database>
      <Database name="GSC232" value="" >
        <!-- name required. type required. base_table, view, or output-->
        <!-- optional attribute UTYPE-->
        <Tables>
          <Table name="PhotoPrimary" type="view">
            <Description>PhotoPrimary Table</Description>
            <!-- meaning of "All" is determined by this entire listing. 
        Meaning of "std" is determined by columns with "std" attribute, set to true. Lack of
        "std" attribute is logically the same as "false"-->
            <Columns>
              <Column name="objID" std="true">
                <Description>object ID, primary key</Description>
                <datatype>bigint</datatype>
                <primary>true</primary>
                <indexed>true</indexed>
              </Column>
              <Column name="gscID2" >
                <Description>Guide Star 2 ID of the object</Description>
                <datatype>string</datatype>
              </Column>
              <Column name="gsc1ID" >
                <Description>Guide Star 1 ID of the object</Description>
                <datatype>string</datatype>
              </Column>
              <Column name="hstID" >
                <Description>HST Operations ID of the object</Description>
                <datatype>string</datatype>
              </Column>
              <Column name="ra" std="true">
                <Description>ra column</Description>
                <unit>degree</unit>
                <datatype>float</datatype>
              </Column>
              <Column name="dec" std="true">
                <Description>dec column</Description>
                <unit>degree</unit>
                <datatype>float</datatype>
              </Column>
              <Column name="epoch">
                <Description>epoch of observation</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="raEpsilon">
                <Description>Error in ra measurement</Description>
                <datatype>real</datatype>
                <unit>arcsec</unit>
              </Column>
              <Column name="decEpsilon">
                <Description>Error in dec measurement</Description>
                <datatype>real</datatype>
                <unit>arcsec</unit>
              </Column>
              <Column name="rapm">
                <Description>Right ascension proper motion</Description>
                <datatype>real</datatype>
                <unit>milli-arcsec per year</unit>
              </Column>
              <Column name="decpm">
                <Description>Declination proper motion</Description>
                <datatype>real</datatype>
                <unit>milli-arcsec per year</unit>
              </Column>
              <Column name="rapmErr">
                <Description>error in right ascension proper motion</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="decpmErr">
                <Description>error in declination proper motion</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="deltaEpoch">
                <Description>delta Epoch for proper motion computation</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="FpgMag">
                <Description>Photographic F Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="FpgMagErr">
                <Description>Error in photographic F Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="FpgMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="JpgMag">
                <Description>Photographic J Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="JpgMagErr">
                <Description>Error in photographic J Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="JpgMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="VMag">
