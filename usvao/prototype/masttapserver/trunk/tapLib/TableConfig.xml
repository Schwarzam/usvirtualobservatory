              </Column>
              <Column name="NMagErr">
                <Description>Error in Photographic IVN Magnitude</Description>
                <datatype>real</datatype>
              </Column>
              <Column name="NMagCode">
                <Description>bandpass code</Description>
                <datatype>int</datatype>
              </Column>
              <Column name="UMag">
