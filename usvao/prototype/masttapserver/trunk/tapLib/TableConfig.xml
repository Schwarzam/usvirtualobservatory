                      <datatype>int</datatype>
                    </Column>
                    <Column name="HTMID">
                      <Description>unique HTM ID</Description>
                      <datatype>bigint</datatype>
                    </Column>
                    <Column name="cx">
                      <Description>Cartesian coordinate x</Description>
                      <datatype>float</datatype>
                    </Column>
                    <Column name="cy">
                      <Description>Cartesian coordinate y</Description>
                      <datatype>float</datatype>
                    </Column>
                    <Column name="cz">
                      <Description>Cartesian coordinate z</Description>
                      <datatype>float</datatype>
                    </Column>-->
              <Column name="gLat">
                <Description>Galactic Latitude</Description>
                <datatype>float</datatype>
              </Column>
              <Column name="gLon">
                <Description>Galactic Longitude</Description>
                <datatype>float</datatype>
              </Column>
              <PositionData>
                <RA Column="ra"/>
                <DEC Column="dec"/>
              </PositionData>
            </Columns>
          </Table>
        </Tables>
        <!-- Can have multiple tables. 
      Tables are logical tables from the client perspective, and may actually be views.-->
      </Database>
        <Database name="HLATest" value="" >
            <!-- name required. type required. base_table, view, or output-->
            <!-- optional attribute UTYPE-->
            <Tables>
                <Table name="hlascience" type="table">
                    <Description>Main HLA Science Table</Description>

                    <!-- meaning of "All" is determined by this entire listing. 
        Meaning of "std" is determined by columns with "std" attribute, set to true. Lack of
        "std" attribute is logically the same as "false"-->
                    <!-- optional attribute UTYPE-->
                  <Columns>
                    <Column name="objid" std="true">
                      <Description>ObjID</Description>
                      <datatype>bigint</datatype>
                      <primary>true</primary>
                      <indexed>true</indexed>
                    </Column>
                    <Column name="ra" std="true">
                      <Description>ra column</Description>
                      <unit>degree</unit>
                      <datatype>float</datatype>
                    </Column>
                    <Column name="dec" std="true">
                      <Description>dec column</Description>
                      <datatype>float</datatype>
                      <unit>degree</unit>
                    </Column>
                    <Column name="level">
                      <Description>Processing level: 1=exposure 2=combined 4=color</Description>
                      <datatype>int</datatype>
                    </Column>
                    <Column name="targetname">
                      <Description>Target Name</Description>
                      <datatype>string</datatype>
                    </Column>
                    <Column name="detector">
                      <Description>Detector</Description>
                      <datatype>string</datatype>
                    </Column>
                    <Column name="aperture">
                      <Description>Instrument aperture or slit</Description>
                      <datatype>string</datatype>
                    </Column>
                    <Column name="numexposures">
                      <Description>Number of exposures combined in this image</Description>
                      <datatype>int</datatype>
                    </Column>
                    <Column name="exposuretime">
                      <Description>Total exposure time</Description>
                      <datatype>float</datatype>
                      <unit>seconds</unit>
                    </Column>
                    <Column name="starttime">
                      <Description>start time of first exposure</Description>
                      <datatype>string (datetime)</datatype>
                    </Column>
                    <Column name="stoptime">
                      <Description>stop time of first exposure</Description>
                      <datatype>string (datetime)</datatype>
                    </Column>
                    <Column name="datasetname">
                      <Description>data set name: HST_propid_visit_detector_spectralelt_exposure</Description>
                      <datatype>string</datatype>
                    </Column>
                    <Column name="prop_id">
                      <Description>HST proposal number</Description>
                      <datatype>int</datatype>
                    </Column>
                    <!--visit number???-->
                    <Column name="pi_last_name">
                      <Description>Last Name of PI</Description>
                      <datatype>string</datatype>
                    </Column>
                    <Column name="mode">
                      <Description>data collection mode</Description>
                      <datatype>string</datatype>
                    </Column>
                    <Column name="dbcreationdate">
                      <Description>date when this dataset was added to the database</Description>
                      <datatype>string (datetime)</datatype>                   
                    </Column>
                    <Column name="wavelength">
                      <Description>Characteristic wavelength for bandpass</Description>
                      <datatype>float</datatype>
                      <unit>0.1nm</unit>
                    </Column>
                    <Column name="instrument">
                      <Description>Instrument Name</Description>
                      <datatype>string</datatype>
                    </Column>
                    <!--format, title, naxis, scale, naxes, crpix, crval, cd_matric, filename-->
                    
                    <PositionData>
                      <RA Column="ra"/>
                      <DEC Column="dec"/>
                    </PositionData>
                  </Columns>
                </Table>
            </Tables>
            <!-- Can have multiple tables. 
      Tables are logical tables from the client perspective, and may actually be views.-->
        </Database>
    </Databases >
</NewConfig>