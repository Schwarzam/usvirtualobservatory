ResourcesTemplate=queryform_resources.html
NoResourcesTemplate=queryform_noresources.html

SourceTable=TestQuery
CMDBase=@VOCLIENT@
CGIBase=@CGI_PATH@
docbase=@URL_PATH@
JS_PATH=@JS_PATH@
CSS_PATH=@CSS_PATH@
CGIProgram=sq.pl
URLAbs=@ABS_PATH@

VOClientDir=@VOCLIENT@

DefaultRadius=0.25
SingleTargetCmd=@VOCLIENT@/vodata -q -V -O -
