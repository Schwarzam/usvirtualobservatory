CombinerXSL=@XSL_RUN@/combiner.xsl
SinglerXSL=@XSL_RUN@/singler.xsl
# ValueXSL=@XSL_RUN@/addValue.xsl

# These may change, so simpler to keep them
# available for updating without reinstalling
VIM=@VIM@
Inventory=@Inventory@
Registry=@Registry@
DataScope=@DataScope@
VOClient=http://iraf-nvo.noao.edu/vo-cli/
