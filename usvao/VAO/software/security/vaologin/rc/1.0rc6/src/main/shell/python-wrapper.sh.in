#! /bin/sh
#
bindir=`dirname $0`
[ -n "$bindir" ] && bindir="$bindir/"
pyscript="$script"
pyscriptpath="${bindir}${pyscript}.py"
python="$exe"
libs="$lib"

if [ -z "$LD_LIBRARY_PATH" ]; then 
    export LD_LIBRARY_PATH=${libs}
else
    export LD_LIBRARY_PATH=${libs}:$LD_LIBRARY_PATH
fi

exec ${python} ${pyscriptpath} "$@"



